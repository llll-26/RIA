<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.student.mapper.BizUserFeedbackMapper">
    
    <resultMap type="BizUserFeedback" id="BizUserFeedbackResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="user_id"    />
        <result property="title"    column="title"    />
        <result property="content"    column="content"    />
        <result property="contact"    column="contact"    />
        <result property="status"    column="status"    />
        <result property="processedAt"    column="processed_at"    />
        <result property="createdAt"    column="created_at"    />
    </resultMap>

    <sql id="selectBizUserFeedbackVo">
        select id, user_id, title, content, contact, status, processed_at, created_at from biz_user_feedback
    </sql>

    <select id="selectBizUserFeedbackList" parameterType="BizUserFeedback" resultMap="BizUserFeedbackResult">
        <include refid="selectBizUserFeedbackVo"/>
        <where>  
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="contact != null  and contact != ''"> and contact = #{contact}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="processedAt != null "> and processed_at = #{processedAt}</if>
            <if test="createdAt != null "> and created_at = #{createdAt}</if>
        </where>
    </select>
    
    <select id="selectBizUserFeedbackById" parameterType="Long" resultMap="BizUserFeedbackResult">
        <include refid="selectBizUserFeedbackVo"/>
        where id = #{id}
    </select>

    <insert id="insertBizUserFeedback" parameterType="BizUserFeedback" useGeneratedKeys="true" keyProperty="id">
        insert into biz_user_feedback
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="title != null and title != ''">title,</if>
            <if test="content != null and content != ''">content,</if>
            <if test="contact != null">contact,</if>
            <if test="status != null">status,</if>
            <if test="processedAt != null">processed_at,</if>
            <if test="createdAt != null">created_at,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="title != null and title != ''">#{title},</if>
            <if test="content != null and content != ''">#{content},</if>
            <if test="contact != null">#{contact},</if>
            <if test="status != null">#{status},</if>
            <if test="processedAt != null">#{processedAt},</if>
            <if test="createdAt != null">#{createdAt},</if>
         </trim>
    </insert>

    <update id="updateBizUserFeedback" parameterType="BizUserFeedback">
        update biz_user_feedback
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="title != null and title != ''">title = #{title},</if>
            <if test="content != null and content != ''">content = #{content},</if>
            <if test="contact != null">contact = #{contact},</if>
            <if test="status != null">status = #{status},</if>
            <if test="processedAt != null">processed_at = #{processedAt},</if>
            <if test="createdAt != null">created_at = #{createdAt},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBizUserFeedbackById" parameterType="Long">
        delete from biz_user_feedback where id = #{id}
    </delete>

    <delete id="deleteBizUserFeedbackByIds" parameterType="String">
        delete from biz_user_feedback where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>